	"sourceCodes": {
		"DDAI/IDDAI.sol": "pragma solidity >=0.4.21 <0.6.0;\n\n interface IDDAI {\n    function mint(\n        address _receiver,\n        address _amount\n    )\n    external\n    returns (uint256);\n\n    function burn(\n        address _receiver,\n        address _amount\n    )\n    external\n    returns (uint256);\n\n    function addRecipe(\n        address _receiver,\n        uint256 _ratio,\n        bytes calldata _data\n    )\n    external\n    returns(bool);\n\n    function removeRecipe(\n        uint256 index\n    )\n    external\n    returns(bool);\n }"
	},
